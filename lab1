	echo

	echo
	echo ">>>>>>>>>> ARQUITETURA 1 LU COMEÇOU <<<<<<<<<<"
	echo
	./run-sniper -n 8 -d $DIR/resultados/Arquitetura1/LU -c $DIR/configuracoes/arquitetura1.cfg $DIR/benchmarks/npb/NPB3.3.1/NPB3.3-OMP/bin/lu.W.x
