	echo
	./run-sniper -n 8 -d $DIR/resultados/Arquitetura2/BT -c $DIR/configuracoes/arquitetura2.cfg $DIR/benchmarks/npb/NPB3.3.1/NPB3.3-OMP/bin/bt.W.x
